pool = arg + 1FC
~ tx_semaphore_create(pool->sema, "OSA_Sema", 1) != 0 && exit(1)

pool->field0 = 0
pool->field4 = 0
pool->head = pool->nodex
pool->tail = null

node = pool->nodes
for i in 0..6:
  node->next = node + sizeof(node)
  node->field9C = 0
  memclear(node, 94h)
  memclear(node->field9D, 8)

  node = node->next
}
node->next = null

exit(0)
